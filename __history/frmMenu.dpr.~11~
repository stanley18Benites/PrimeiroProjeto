program frmMenu;

uses
  Vcl.Forms,
  uMenu in 'uMenu.pas' {frmMenuRecibo},
  uCadastro in 'uCadastro.pas' {frmCadastro},
  uRecibo in 'uRecibo.pas' {frmRecibo},
